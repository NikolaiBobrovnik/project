    "react-loadable": "^5.4.0",
    "react-masonry-component": "^6.2.0",
    "react-nested-status": "0.2.1",
    "react-recaptcha-v3": "http://gitlab.aic.ru:81/Artur_Rutrovich/react-recaptcha-v3.git",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
