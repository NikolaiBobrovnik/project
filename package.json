    "webpack-manifest-plugin": "1.3.2",
    "webpack-node-externals": "1.7.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "node scripts/start.js",
    "start-ssr": "node scripts/start-ssr.js",
    "serve": "node ./build/server/bundle.js",
    "build": "node scripts/build.js",
    "build-server": "node scripts/build-server.js",
    "build-ssr": "set REACT_APP_SSR=true&& npm run build && npm run build-server",
    "nodemon": "nodemon --watch build/server build/server/bundle.js build/server/bundle.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "3.3.7",
    "express": "4.16.3",
    "immutable": "^3.8.2",
    "jquery": "^3.3.1",
